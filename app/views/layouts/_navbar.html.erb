<header>
  <a href="/">タスク</a>
<div>
  <ul>
    <% if logged_in? %>
        <ul>
          <li><%= link_to 'My profile', root_path(current_user) %></li>
          <li><%= link_to 'Logout', logout_path, method: :delete %></li>
        </ul>
      </li>
    <% else %>
      <li><%= link_to '今すぐ新規登録', signup_path %></li>
      <li><%= link_to 'ログイン', login_path %></li>
    <% end %>
  </ul>
</div>
</header>