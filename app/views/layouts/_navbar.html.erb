<header>
    <a href="/">Tasklist</a>
    <div>
      <ul>
        <% if logged_in? %>
        <ul>
          <li><%= link_to 'My profile', root_path(current_user) %></li>
          <li><%= link_to 'Logout', logout_path, method: :delete %></li>
        </ul>
        <% else %>
          <li><%= link_to 'signup', signup_path %></li>
          <li><%= link_to 'login', login_path %></li>
        <% end %>
      </ul>
    </div>
</header>