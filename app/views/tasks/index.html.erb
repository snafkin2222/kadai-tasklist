<% if logged_in? %>
<h1>タスク一覧</h1>
      <tr>
        <th>id</th>
        <th>タスク</th>
        <th>ステータス</th>
      </tr>
     <% @tasks.each do |task| %>
    <tr>
      <li><%= link_to task.id, task %> : <%= task.status %> &gt; <%= task.content %></li>
    </tr>
    <% end %>
    <%= link_to '新規タスクの投稿', new_task_path %> 
  
<% else %>
<div>
  <div>
    <h1>Your Tasklists</h1>

     <%= link_to 'ログイン',login_path %>
      <%= link_to '今すぐ新規ユーザ登録',signup_path %>
  </div>
</div>
<% end %>